set visualbell
" register leader
let mapleader = "\"

nnoremap <leader>g :action Git.Menu<CR>
nnoremap <leader>s :action GotoSymbol<CR>

nnoremap <leader>f :action GotoFile<CR>
nnoremap <leader>F :action RecentFiles<CR>

nnoremap <leader>a :action GotoAction<CR>
nnoremap <leader>c :action GotoClass<CR>

nnoremap [e :action GotoError<CR>
nnoremap ]e :action GotoPreviousError<CR>

nnoremap <leader>r :action ChooseRunConfiguration<CR>
nnoremap <leader>R :action RunConfiguration<CR>

nnoremap <leader>q :action HideAllWindows<CR>

nnoremap <leader>t :action ActivateTerminalToolWindow<CR>

